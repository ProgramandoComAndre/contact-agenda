<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval" android:thickness="5dp">
            <size
                android:width="20dp"
                android:height="20dp">
            </size>
            <stroke
                android:color="@color/black"
                android:width="1dp"></stroke>

        </shape>
    </item>

    <item android:state_enabled="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval" android:thickness="5dp" android:tint="@color/black">
            <size
                android:width="20dp"
                android:height="20dp">
            </size>
            <stroke
                android:color="@color/black"
                android:width="1dp"></stroke>

        </shape>
    </item>
</selector>
